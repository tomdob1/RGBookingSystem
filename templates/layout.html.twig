  <body>
  {% set page = app.request.get('_route') %}


    <!-- Fixed navbar -->
    <nav class="navbar navbar-default navbar-fixed-top"  style="color:#003865!important;">
      <div class="container"><a class="navbar-brand" href="/">
          <img style="width:100px; height: 40px; margin: -10px" src="{{ absolute_url(asset('https://www.rewardgateway.com/hubfs/05-client-images/homepage-logos/rg-logo-social.png')) }}"></a>
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>


        </div>
        <div id="navbar" class="navbar-collapse collapse" >
          <ul class="nav navbar-nav" style="color:#003865!important;">
            <li {% if 'homepage' in page %} class="active"{% endif %}><a href="/" style="color:#003865!important;">Home</a></li>
            <li {% if 'bookseat' in page %} class="active"{% endif %}><a href="/bookSeat" style="color:#003865!important;">Booking</a></li>
            <li {% if 'addemployee' in page %} class="active"{% endif %}><a href="/addEmployee" style="color:#003865!important;">Add Employee</a></li>
            <li {% if 'addoffice' in page %} class="active"{% endif %}><a href="/addOffice" style="color:#003865!important;">Add Office</a></li>

          </ul>
        </div><!--/.nav-collapse -->
      </div>
